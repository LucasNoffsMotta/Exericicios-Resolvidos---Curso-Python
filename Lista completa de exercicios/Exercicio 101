from datetime import datetime

def voto(ano):
    idade = 2024 - ano
    if idade >= 18:
        return f'Com {idade} anos: voto obrigatorio.'
    if idade >= 16:
        return f'Com {idade} anos: voto opcional.'
    else:
        return f'Com {idade} anos: voto nao vota.'


#Main:

print('-'*30)
ano_nasc = int(input('Digite aqui o ano de seu nascimento: '))
print(voto(ano_nasc))


